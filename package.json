{
  "name": "kahla",
  "version": "4.5.0",
  "description": "Kahla is a cross-platform business messaging app.",
  "author": "Aiursoft <service@aiursoft.com> (https://www.aiursoft.cn/)",
  "contributors": [
    "Anduin@aiursoft.com",
